/* components/nineImg/cmp.wxss */
.container{
    width: 700rpx;
}
.img-container{
    position: relative;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    overflow: hidden;
    width: 700rpx;
    height: 700rpx;
}
.img{
    width: 31%;
    height: 31%;
}
.text-container{
    padding: 0 30rpx;
    font-size: 40rpx;
    font-weight: 400;
    letter-spacing: 2rpx;
}
.img-num{
    position: absolute;
    right: 80rpx;
    bottom: 80rpx;
    padding: 5rpx 10rpx;
    background-color: rgba(0,0,0,.7);
    color: #fff;
    font-size: 24rpx;
}
